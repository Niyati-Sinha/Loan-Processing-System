create database demo;
use demo;
CREATE TABLE `users` (
   `id` int(3) NOT NULL,
   `first_name` varchar(20) DEFAULT NULL,
   `last_name` varchar(20) DEFAULT NULL,
   `username` varchar(250) DEFAULT NULL,
   `password` varchar(20) DEFAULT NULL,
   PRIMARY KEY (`id`)
);
select *from users;
INSERT INTO `demo`.`users` (`id`, `first_name`, `last_name`, `username`, `password`) VALUES ('1', 'niyati', 'sinha', 'niyati-sinha', '123');